<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/nav_main"
        app:startDestination="@id/mainFragment">

    <fragment
            android:id="@+id/mainFragment"
            android:name="com.adedom.android.presentation.main.MainFragment"
            android:label="MainFragment"
            tools:layout="@layout/fragment_main">
        <action
                android:id="@+id/action_mainFragment_to_settingFragment"
                app:destination="@id/settingFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_mainFragment_to_aboutFragment"
                app:destination="@id/aboutFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_mainFragment_to_rankFragment"
                app:destination="@id/rankFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_mainFragment_to_singleFragment"
                app:destination="@id/singleFragment" />
        <action
                android:id="@+id/action_mainFragment_to_missionFragment"
                app:destination="@id/missionFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_mainFragment_to_roomFragment"
                app:destination="@id/roomFragment" />
    </fragment>
    <fragment
            android:id="@+id/changePasswordFragment"
            android:name="com.adedom.android.presentation.changepassword.ChangePasswordFragment"
            android:label="Change password"
            tools:layout="@layout/fragment_change_password" />
    <fragment
            android:id="@+id/changeImageFragment"
            android:name="com.adedom.android.presentation.changeimage.ChangeImageFragment"
            android:label="Change image profile"
            tools:layout="@layout/fragment_change_image" />
    <fragment
            android:id="@+id/changeProfileFragment"
            android:name="com.adedom.android.presentation.changeprofile.ChangeProfileFragment"
            android:label="Change profile"
            tools:layout="@layout/fragment_change_profile" />
    <fragment
            android:id="@+id/settingFragment"
            android:name="com.adedom.android.presentation.setting.SettingFragment"
            android:label="Setting"
            tools:layout="@layout/fragment_setting">
        <action
                android:id="@+id/action_settingFragment_to_changePasswordFragment"
                app:destination="@id/changePasswordFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_settingFragment_to_changeImageFragment"
                app:destination="@id/changeImageFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_settingFragment_to_changeProfileFragment"
                app:destination="@id/changeProfileFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
        <action
                android:id="@+id/action_settingFragment_to_authActivity"
                app:destination="@id/authActivity" />
        <action
                android:id="@+id/action_settingFragment_to_playerProfileFragment"
                app:destination="@id/playerProfileFragment"
                app:enterAnim="@anim/slide_in_right"
                app:exitAnim="@anim/slide_out_left"
                app:popEnterAnim="@anim/slide_in_left"
                app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
            android:id="@+id/aboutFragment"
            android:name="com.adedom.android.presentation.about.AboutFragment"
            android:label="About"
            tools:layout="@layout/fragment_about" />
    <activity
            android:id="@+id/authActivity"
            android:name="com.adedom.android.presentation.auth.AuthActivity"
            android:label="activity_auth"
            tools:layout="@layout/activity_auth" />
    <fragment
            android:id="@+id/rankFragment"
            android:name="com.adedom.android.presentation.rank.RankFragment"
            android:label="Rank"
            tools:layout="@layout/fragment_rank" />
    <fragment
            android:id="@+id/singleFragment"
            android:name="com.adedom.android.presentation.single.SingleFragment"
            android:label="SingleFragment"
            tools:layout="@layout/fragment_single">
        <action
                android:id="@+id/action_singleFragment_to_backpackFragment"
                app:destination="@id/backpackFragment" />
    </fragment>
    <fragment
            android:id="@+id/backpackFragment"
            android:name="com.adedom.android.presentation.backpack.BackpackFragment"
            android:label="BackpackFragment"
            tools:layout="@layout/fragment_backpack" />
    <fragment
            android:id="@+id/missionFragment"
            android:name="com.adedom.android.presentation.mission.MissionFragment"
            android:label="Mission"
            tools:layout="@layout/fragment_mission" />
    <action
            android:id="@+id/action_global_authActivity"
            app:destination="@id/authActivity" />
    <fragment
            android:id="@+id/roomFragment"
            android:name="com.adedom.android.presentation.room.RoomFragment"
            android:label="RoomFragment"
            tools:layout="@layout/fragment_room">
        <action
                android:id="@+id/action_roomFragment_to_createRoomFragment"
                app:destination="@id/createRoomFragment" />
        <action
                android:id="@+id/action_roomFragment_to_roomInfoFragment"
                app:destination="@id/roomInfoFragment" />
    </fragment>
    <fragment
            android:id="@+id/createRoomFragment"
            android:name="com.adedom.android.presentation.createroom.CreateRoomFragment"
            android:label="CreateRoomFragment"
            tools:layout="@layout/fragment_create_room">
        <action
                android:id="@+id/action_createRoomFragment_to_roomInfoFragment"
                app:destination="@id/roomInfoFragment"
                app:popUpTo="@id/createRoomFragment"
                app:popUpToInclusive="true" />
    </fragment>
    <fragment
            android:id="@+id/roomInfoFragment"
            android:name="com.adedom.android.presentation.roominfo.RoomInfoFragment"
            android:label="RoomInfoFragment"
            tools:layout="@layout/fragment_room_info">
        <action
                android:id="@+id/action_roomInfoFragment_to_multiFragment"
                app:destination="@id/multiFragment"
                app:popUpTo="@id/roomInfoFragment"
                app:popUpToInclusive="true" />
    </fragment>
    <fragment
            android:id="@+id/playerProfileFragment"
            android:name="com.adedom.android.presentation.playerprofile.PlayerProfileFragment"
            android:label="Player profile"
            tools:layout="@layout/fragment_player_profile" />
    <fragment
            android:id="@+id/multiFragment"
            android:name="com.adedom.android.presentation.multi.MultiFragment"
            android:label="MultiFragment"
            tools:layout="@layout/fragment_multi" />
</navigation>
